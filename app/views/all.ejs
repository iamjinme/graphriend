<div class="table-container">
  <table class="table table-striped table-hover">
    <h3 class="title text-right">All users</h3>
    <thead>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Member since</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(function(friend) { %>
        <% if(friend.username !== user.username) { %>
          <tr>
            <td>
              <figure class="avatar">
                <img src="<%= '//api.adorable.io/avatar/100/' + friend.username %>" />
              </figure>
            </td>
            <td><%= friend.name %></td>
            <td><%= fromNow(friend.date) %></td>
            <td><a id="<%= friend.username %>" class="btn btn-link btn-add tooltip" data-tooltip="Add friend"><i class="fa fa-plus"></i></a></td>
          </tr>
        <% } %>
      <% }) %>
    </tbody>
  </table>
</div>
<script src="/public/js/all.js"></script>
