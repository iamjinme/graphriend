<div class="table-container">
  <table class="table table-striped table-hover">
    <h3 class="title text-right">
      <a id="btn-graph" class="btn btn-link btn-view tooltip" data-tooltip="Graph nodes">
        <i class="fa fa-2x fa-code-fork"></i>
      </a>
      My friends
    </h3>
    <thead>
      <tr>
        <th></th>
        <th>Name</th>
        <th>Member since</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% friends.forEach(function(friend) { %>
      <tr>
        <td>
          <figure class="avatar">
            <img src="<%= '//api.adorable.io/avatar/100/' + friend.username %>" />
          </figure>
        </td>
        <td><%= friend.name %></td>
        <td><%= fromNow(friend.date) %></td>
        <td><a id="<%= friend.username %>" class="btn btn-link btn-view tooltip" data-tooltip="View friends"><i class="fa fa-users"></i></a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
